<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MeetingHelper.Views.SearchResults">
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="ShareB" Text="مشاركة" Clicked="ToolbarItem_Clicked"/>
        <ToolbarItem x:Name="RandomB" Text="اختيار عشوائي" Clicked="RandomB_Clicked"/>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <ListView RowHeight="80" ItemSelected="Results_ItemSelected" x:Name="Results" HorizontalOptions="Fill" VerticalOptions="FillAndExpand"/>
    </ContentPage.Content>
</ContentPage>